	1.2 mv A-LOAM catkin_ws/src/
	1.3 cd catkin_ws
	1.4 catkin_make
	1.5 source devel/setup.bash
	1.6 roslaunch aloam_velodyne aloam_velodyne_VLP_16.launch
	1.7 rosbag play ***.bag


